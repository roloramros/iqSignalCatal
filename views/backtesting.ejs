          else if (a.Resultado === "‚ùå") perdido++;
          else if (a.Resultado === "‚ö†Ô∏è") noCumplida++;
        });

        Swal.fire({
          title: 'üìä Resumen del Backtesting',
          html: `
        <div style="text-align:left; font-size:16px;">
            <b>‚úÖ Ganado directo:</b> ${ganadoDirecto}<br>
