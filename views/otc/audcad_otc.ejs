                const horaStr = velaActual.start_time.toString();
                const minuto = horaStr.split(':')[1] || '00';
                const digito = minuto % 10;
                coloresDigitos[digito] = color;
                analizarVelaPorDigito(velaActual);
            }
            esperarNuevaVela();
        });





